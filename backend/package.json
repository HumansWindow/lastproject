{
  "name": "alivehuman-backend",
  "version": "0.1.0",
  "description": "AliveHuman Backend Application",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "node install-local.js",
    "start": "node --max-old-space-size=4096 dist/main",
    "start:local": "node start-app.js",
    "start:dev": "node --max-old-space-size=4096 node_modules/.bin/nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:cov": "jest --coverage --config jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:file": "jest --detectOpenHandles --forceExit --config jest.config.js",
    "fix-deps": "bash fix-deps.sh",
    "clean-install": "rimraf node_modules && npm cache clean --force && npm install",
    "install:jest-types": "npm install --save-dev @types/jest && tsc --build",
    "start:dev:alt": "PORT=3002 nest start --watch",
    "kill-port": "../kill-port.sh 3001",
    "start:clean": "npm run kill-port && npm run start:dev",
    "start:optimized": "./start-optimized.sh",
    "start:debug:memory": "NODE_OPTIONS='--max-old-space-size=4096 --inspect' nest start --debug --watch",
    "start:memory-safe": "./start-memory-safe.sh",
    "migration:create": "typeorm migration:create",
    "migration:run": "nest build && typeorm migration:run -d dist/database/data-source",
    "migration:revert": "typeorm migration:revert -d dist/database/data-source",
    "fix:jest-types": "npm install --save-dev @types/jest ts-jest jest @jest/globals && tsc --build --clean",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@ethersproject/units": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/websockets": "^10.2.10",
    "@solana/web3.js": "^1.98.0",
    "@types/express-session": "^1.18.1",
    "bcrypt": "^5.1.0",
    "bip32": "^2.0.6",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.4.7",
    "ed25519-hd-key": "^1.3.0",
    "ethers": "^5.7.2",
    "express-session": "^1.18.1",
    "helmet": "^7.2.0",
    "isomorphic-fetch": "^3.0.0",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.11",
    "nestjs-real-ip": "^3.0.1",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "rate-limiter-flexible": "^2.4.2",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^4.6.3",
    "tiny-secp256k1": "^2.2.3",
    "typeorm": "^0.3.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@nestjs/typeorm": "^8.1.4",
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-parser": "^1.4.8",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^28.1.4",
    "@types/node": "^20.17.30",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "better-sqlite3": "^11.9.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ignore-loader": "^0.1.2",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  }
}
